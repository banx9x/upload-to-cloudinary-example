<h1>Upload to Cloudinary Example</h1>

<form action="/upload" method="POST" enctype="multipart/form-data">
    <label for="file">Choose file to upload</label>
    <input type="file" name="file" id="file" />
    <button>Upload</button>
</form>

{{#if result}}

    <div>
        <span>Result:</span>

        <pre>
            <code>
{{result}}
            </code>
        </pre>
    </div>
{{else if error}}
    <p>{{error}}</p>
{{/if}}